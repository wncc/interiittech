<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.social"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Form"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="com.example.social.FORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Map"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="com.example.social.MAP" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Map2"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="com.example.social.MAP2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
           <activity
            android:name=".OurViewClient"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="com.example.social.OURVIEWCLIENT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        
        
        
        
    </application>

</manifest>